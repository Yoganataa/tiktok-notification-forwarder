generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model UserMapping {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  channelId String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([username])
}

model AccessControl {
  userId    String   @id
  role      String
  addedBy   String
  createdAt DateTime @default(now())
}
