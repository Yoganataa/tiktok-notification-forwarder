{
  "name": "tiktok-notification-forwarder",
  "version": "2.2.0",
  "description": "Production-ready Discord bot to forward TikTok notifications",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && cp -r src/core/database/migrations dist/core/database/",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "discord",
    "tiktok",
    "bot",
    "postgresql"
  ],
  "author": "yoganataa",
  "license": "MIT",
  "dependencies": {
    "@sapphire/decorators": "^6.2.0",
    "@sapphire/framework": "^5.5.0",
    "@sapphire/pieces": "^4.4.1",
    "@sapphire/plugin-logger": "^4.1.0",
    "@sapphire/plugin-subcommands": "^7.0.1",
    "@types/glob": "^8.1.0",
    "async-retry": "^1.3.3",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.5.0",
    "cheerio": "^1.2.0",
    "commander": "^14.0.2",
    "crypto-js": "^4.2.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "glob": "^13.0.0",
    "https-proxy-agent": "^7.0.6",
    "jsdom": "^27.4.0",
    "metadownloader": "^1.0.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "qs": "^6.14.2",
    "socks-proxy-agent": "^8.0.5",
    "systeminformation": "^5.21.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "youtube-dl-exec": "^3.0.29",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^25.0.1",
    "@types/pg": "^8.16.0",
    "@types/qs": "^6.14.0",
    "@types/systeminformation": "^3.23.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "requiredEnv": [
    "DISCORD_TOKEN",
    "OWNER_ID",
    "CLIENT_ID",
    "DATABASE_URL"
  ]
}
